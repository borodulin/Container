{
    "name": "borodulin/container",
    "description": "PSR Container",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Borodulin",
            "email": "borodulin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0",
        "psr/cache": "^1.0",
        "opis/closure": "^3.5",
        "psr/simple-cache": "^1.0",
        "borodulin/finder": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.23",
        "phpunit/phpcov": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Borodulin\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Borodulin\\Container\\Tests\\": "tests/"
        }
    }
}
